{
    "name": "catchlion",
    "private": true,
    "version": "0.0.0",
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "apps/*",
        "physics/*",
        "animations/*",
        "scene/*",
        "gui/*"
    ],
    "scripts": {
        "gen": "bun gen/index.ts",
        "dev": "bunx --bun vite",
        "serve": "bun --watch ./server/index.ts",
        "build": "bunx --bun vite build ",
        "dev:clone": "bunx --bun vite physics/003.clone",
        "build:clone": "bunx --bun vite build physics/003.clone",
        "dev:ottugi": "bunx --bun vite physics/004.ottugi",
        "build:ottugi": "bunx --bun vite build physics/004.ottugi",
        "dev:instances": "bunx --bun vite physics/005.instances",
        "build:instances": "bunx --bun vite build physics/005.instances",
        "dev:Pendulum": "bunx --bun vite physics/006.Pendulum",
        "build:Pendulum": "bunx --bun vite build physics/006.Pendulum",
        "dev:raycast1": "bunx --bun vite physics/007.raycast1",
        "build:raycast1": "bunx --bun vite build physics/007.raycast1",
        "dev:observable": "bunx --bun vite scene/000.observable",
        "build:observable": "bunx --bun vite build scene/000.observable",
        "dev:drag": "bunx --bun vite scene/001.drag",
        "build:drag": "bunx --bun vite build scene/001.drag",
        "dev:animals": "bunx --bun vite apps/000.animals",
        "build:animals": "bunx --bun vite build apps/000.animals",
        "dev:animal2": "bunx --bun vite apps/001.animal2",
        "build:animal2": "bunx --bun vite build apps/001.animal2",
        "dev:slider": "bunx --bun vite gui/000.slider",
        "build:slider": "bunx --bun vite build gui/000.slider",
        "dev:lobby": "bunx --bun vite apps/002.lobby",
        "build:lobby": "bunx --bun vite build apps/002.lobby",
        "dev:cc": "bunx --bun vite animations/000.cc",
        "build:cc": "bunx --bun vite build animations/000.cc",
        "dev:arccamera": "bunx --bun vite scene/002.arccamera",
        "build:arccamera": "bunx --bun vite build scene/002.arccamera",
        "dev:group": "bunx --bun vite gui/001.group",
        "build:group": "bunx --bun vite build gui/001.group",
        "dev:flower": "bunx --bun vite scene/003.flower",
        "build:flower": "bunx --bun vite build scene/003.flower",
        "dev:server": "bunx --bun vite apps/003.server",
        "build:server": "bunx --bun vite build apps/003.server",
        "dev:observer": "bunx --bun vite scene/004.observer",
        "build:observer": "bunx --bun vite build scene/004.observer",
        "dev:resume": "bunx --bun vite apps/004.resume",
        "build:resume": "bunx --bun vite build apps/004.resume"
    },
    "dependencies": {
        "@babylonjs/core": "^6.35.0",
        "@babylonjs/gui": "^6.37.1",
        "@babylonjs/havok": "^1.3.1-alpha.2",
        "@babylonjs/inspector": "^6.35.0",
        "ammojs-typed": "^1.0.6",
        "babylonjs-serializers": "^6.36.1",
        "earcut": "^2.2.4",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/prompts": "^2.4.9",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "eslint": "~8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "prettier": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^5.0.8"
    }
}